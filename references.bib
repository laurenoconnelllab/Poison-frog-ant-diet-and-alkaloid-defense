@article{navas_implications_1996,
	title = {Implications of microhabitat selection and patterns of activity on the thermal ecology of high elevation neotropical anurans},
	volume = {108},
	issn = {0029-8549, 1432-1939},
	url = {http://link.springer.com/10.1007/BF00329034},
	doi = {10.1007/BF00329034},
	abstract = {This paper focuses on whether microhabitat selection reduces exposure to cold temperatures in highelevation tropical amphibians. I studied the microhabitat associations and thermal ecology of four anurans (Colostethus subpunctatus, Atelopus sp. nov., Eleutherodactylus bogotensis, and Hyla labialis) that live in the Andes of central Colombia. I compared two populations (3500 m and 2900 m) to evaluate the impact of a change in elevation on microhabitat thermal regimes and frog behavior. Ambient temperatures of frequently used microhabitats (soil surface, retreat sites, bogs, and ponds) were significantly lower at 3500 m. However, for each species the microhabitat associations and general patterns of activity were similar at both elevations. Body temperatures of active frogs were mainly determined by the interaction between microhabitat selection and time of the day, therefore frogs at 3500 m had lower activity temperatures than conspecifics at 2900 m. Terrestrial and diurnal anurans (Atelopus sp. nov.) experienced highly variable activity temperatures. Frogs associated with water bodies (C. subpunctatus, and H. labialis) experienced more moderate and less variable temperatures. Terrestrial, nocturnal frogs (E. bogotensis) exhibited the lowest activity temperatures, and their thermal ecology was strongly affected by increase in altitude. The physical nature of high tropical elevations limits the opportunities for behavioral thermoregulation in amphibians, and increases the importance of physiological adjustments to tolerate a wide thermal range. This may partially explain why only a small number of amphibian species, representing genera that are much more diverse at lower elevations, have been able to exploit these environments.},
	pages = {617--626},
	number = {4},
	journaltitle = {Oecologia},
	shortjournal = {Oecologia},
	author = {Navas, Carlos A.},
	urldate = {2021-07-18},
	date = {1996-12},
	langid = {english},
	file = {Navas - 1996 - Implications of microhabitat selection and pattern.pdf:/Users/camilorl/Zotero/storage/9Q34KVIG/Navas - 1996 - Implications of microhabitat selection and pattern.pdf:application/pdf},
}

@article{navas_thermal_2008,
	title = {Thermal relationships and exercise physiology in anuran amphibians: Integration and evolutionary implications},
	volume = {151},
	issn = {10956433},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1095643307015115},
	doi = {10.1016/j.cbpa.2007.07.003},
	shorttitle = {Thermal relationships and exercise physiology in anuran amphibians},
	abstract = {Thermal and water balance are coupled in anurans, and species with particularly permeable skin avoid overheating more effectively than minimizing variance of body temperature. In turn, temperature affects muscle performance in several ways, so documenting the mean and variance of body temperature of active frogs can help explain variation in behavioral performance. The two types of activities studied in most detail, jumping and calling, differ markedly in duration and intensity, and there are distinct differences in the metabolic profile and fiber type of the supporting muscles. Characteristics of jumping and calling also vary significantly among species, and these differences have a number of implications that we discuss in some detail throughout this paper. One question that emerges from this topic is whether anuran species exhibit activity temperatures that match the temperature range over which they perform best. Although this seems the case, thermal preferences are variable and may not necessarily reflect typical activity temperatures. The performance versus temperature curves and the thermal limits for anuran activity reflect the thermal ecology of species more than their systematic position. Anuran thermal physiology, therefore, seems to be phenotypically plastic and susceptible to adaptive evolution. Although generalizations regarding the mechanistic basis of such adjustments are not yet possible, recent attempts have been made to reveal the mechanistic basis of acclimation and acclimatization.},
	pages = {344--362},
	number = {3},
	journaltitle = {Comparative Biochemistry and Physiology Part A: Molecular \& Integrative Physiology},
	shortjournal = {Comparative Biochemistry and Physiology Part A: Molecular \& Integrative Physiology},
	author = {Navas, Carlos A. and Gomes, Fernando R. and Carvalho, José Eduardo},
	urldate = {2021-07-18},
	date = {2008-11},
	langid = {english},
	file = {Navas et al. - 2008 - Thermal relationships and exercise physiology in a.pdf:/Users/camilorl/Zotero/storage/P9ZSTE87/Navas et al. - 2008 - Thermal relationships and exercise physiology in a.pdf:application/pdf},
}

@article{nowicki_evidence_2022,
	title = {{EVIDENCE} {FOR} {EMPATHY} {IN} {PAIR} {BONDING} {POISON} {FROGS}},
	url = {http://biorxiv.org/content/early/2022/09/26/2022.09.25.509360.abstract},
	doi = {10.1101/2022.09.25.509360},
	abstract = {Empathy is critical to individual and societal wellbeing in humans. More than a century ago, Charles Darwin hypothesized that the empathy-like phenotype is a phylogenetically widespread phenomenon. This idea remains contentious, partly due to few investigations among non-mammalian vertebrates. We provide evidence for Darwin’s hypothesis by discovering key empathetic features, emotional contagion and ingroup bias, in pair bonding Ranitomeya imitator (Mimetic) poison frogs. We found that the corticosterone level of males appears to positively correlate with female partners across various conditions, including cohabitation and during experimental manipulation. This response is selective towards female partners relative to familiarized female non-partners and is irrespective of partnership longevity or lifetime reproductive output. However, hormonal state matching is not reflected behaviorally, indicating that behavioral state matching can be an unreliable proxy for emotional contagion. These results constitute the first evidence for emotional contagion in an amphibian. Together with findings in other taxa, they suggest empathy is evolutionarily widespread among social vertebrates.Competing Interest {StatementThe} authors have declared no competing interest.},
	pages = {2022.09.25.509360},
	journaltitle = {{bioRxiv}},
	author = {Nowicki, Jessica P. and Rodríguez, Camilo and Lee, Julia C. and Goolsby, Billie C. and Yang, Chen and Cleland, Thomas A. and O’Connell, Lauren A.},
	date = {2022-01-01},
}

@article{rodriguez_androgen_2022,
	title = {Androgen responsiveness to simulated territorial intrusions in Allobates femoralis males: Evidence supporting the challenge hypothesis in a territorial frog},
	volume = {326},
	issn = {00166480},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0016648022000715},
	doi = {10.1016/j.ygcen.2022.114046},
	shorttitle = {Androgen responsiveness to simulated territorial intrusions in Allobates femoralis males},
	abstract = {Territoriality has been widely described across many animal taxa, where the acquisition and defence of a ter­ ritory are critical for the fitness of an individual. Extensive evidence suggests that androgens are involved in the modulation of territorial behaviours in male vertebrates. Short-term increase of androgen following a territorial encounter appears to favour the outcome of a challenge. The “Challenge Hypothesis” proposed by Wingfield and colleagues outlines the existence of a positive feedback relationship between androgen and social challenges (e. g., territorial intrusions) in male vertebrates. Here we tested the challenge hypothesis in the highly territorial poison frog, Allobates femoralis, in its natural habitat by exposing males to simulated territorial intrusions in the form of acoustic playbacks. We quantified repeatedly androgen concentrations of individual males via a noninvasive water-borne sampling approach. Our results show that A. femoralis males exhibited a positive behav­ ioural and androgenic response after being confronted to simulated territorial intrusions, providing support for the Challenge Hypothesis in a territorial frog.},
	pages = {114046},
	journaltitle = {General and Comparative Endocrinology},
	shortjournal = {General and Comparative Endocrinology},
	author = {Rodríguez, Camilo and Fusani, Leonida and Raboisson, Gaëlle and Hödl, Walter and Ringler, Eva and Canoine, Virginie},
	urldate = {2023-02-14},
	date = {2022-09},
	langid = {english},
	file = {Rodríguez et al. - 2022 - Androgen responsiveness to simulated territorial i.pdf:/Users/camilorl/Zotero/storage/WI8KNKQC/Rodríguez et al. - 2022 - Androgen responsiveness to simulated territorial i.pdf:application/pdf},
}

