@article{burkett_oxytocin-dependent_2016-1,
	title = {Oxytocin-dependent consolation behavior in rodents},
	volume = {351},
	issn = {0036-8075, 1095-9203},
	url = {https://www.science.org/doi/10.1126/science.aac4785},
	doi = {10.1126/science.aac4785},
	abstract = {Let me comfort you
            
              Consolation behavior promotes stress reduction of one by another. We know that consolation occurs in humans and apes. Burkett
              et al.
              observed that within a pair of monogamous prairie voles, an unstressed partner increased its grooming of a stressed partner. Furthermore, the unstressed partner matched the stressed partner in its stress hormone response. Thus, consolation may be more common than assumed in animals, and prairie voles may prove a useful model for understanding the physical and neural mechanisms underlying consolation behavior.
            
            
              Science
              , this issue p.
              375
            
          , 
            Prairie voles console their family members after stressful events.
          , 
            
              Consolation behavior toward distressed others is common in humans and great apes, yet our ability to explore the biological mechanisms underlying this behavior is limited by its apparent absence in laboratory animals. Here, we provide empirical evidence that a rodent species, the highly social and monogamous prairie vole (
              Microtus ochrogaster
              ), greatly increases partner-directed grooming toward familiar conspecifics (but not strangers) that have experienced an unobserved stressor, providing social buffering. Prairie voles also match the fear response, anxiety-related behaviors, and corticosterone increase of the stressed cagemate, suggesting an empathy mechanism. Exposure to the stressed cagemate increases activity in the anterior cingulate cortex, and oxytocin receptor antagonist infused into this region abolishes the partner-directed response, showing conserved neural mechanisms between prairie vole and human.},
	pages = {375--378},
	number = {6271},
	journaltitle = {Science},
	shortjournal = {Science},
	author = {Burkett, J. P. and Andari, E. and Johnson, Z. V. and Curry, D. C. and De Waal, F. B. M. and Young, L. J.},
	urldate = {2024-05-23},
	date = {2016-01-22},
	langid = {english},
	file = {Burkett et al. - 2016 - Oxytocin-dependent consolation behavior in rodents.pdf:/Users/camilorl/Zotero/storage/K93LJ8CT/Burkett et al. - 2016 - Oxytocin-dependent consolation behavior in rodents.pdf:application/pdf},
