@article{betancourth-cundar_genetic_2020,
	title = {Genetic differentiation and overexploitation history of the critically endangered Lehmann’s Poison Frog: Oophaga lehmanni},
	volume = {21},
	issn = {1566-0621, 1572-9737},
	url = {https://link.springer.com/10.1007/s10592-020-01262-w},
	doi = {10.1007/s10592-020-01262-w},
	shorttitle = {Genetic differentiation and overexploitation history of the critically endangered Lehmann’s Poison Frog},
	abstract = {Species conservation with fragmented and endangered populations must be based on a prior and thorough knowledge of the structure and population dynamics. Oophaga lehmanni is a dendrobatid species endemic of Colombia and is restricted to its type locality. This species has a fragmented distribution and is considered as critically endangered mainly due to habitat destruction and overexploitation. Oophaga lehmanni exhibits phenotypic variation in the dorsal color pattern (red and yellow morphs). We reconstructed the overexploitation history that this species has faced in the last 40 years. In addition, we collected genetic and morphological data for the first time in natural populations to describe genetic diversity between and within populations, and to evaluate morphological and genetic differences between red and yellow morphs. Overexploitation data suggest that more than 80.000 (Min = 60.047–Max = 102.236) frogs were extracted from the field in the last four decades, probably generating the local extirpation or population decline from the type locality. Genetic data showed reduced genetic diversity. Observed heterozygosity (mean ± s.d. = 0.599 ± 0.165) is lower than expected (mean ± s.d. = 0.867 ± 0.082). We did not find differences in body size and heterozygosity between the two morphs; however, individuals analyzed were assigned to two genetic clusters, which corresponded to the O. lehmanni-yellow and O. lehmanni-red. In addition, F­ {ST} (0.209) and Nei genetic distance (0.18) values indicated genetic differentiation between the two morphs; therefore, red and yellow morphs should be treated as independent management units. This information will help to define appropriate and long-term conservation units, as a useful tool to mitigate the extinction risk of this species.},
	pages = {453--465},
	number = {3},
	journaltitle = {Conservation Genetics},
	shortjournal = {Conserv Genet},
	author = {Betancourth-Cundar, Mileidy and Palacios-Rodríguez, Pablo and Mejía-Vargas, Daniel and Paz, Andrea and Amézquita, Adolfo},
	urldate = {2021-10-20},
	date = {2020-06},
	langid = {english},
	file = {Betancourth-Cundar et al. - 2020 - Genetic differentiation and overexploitation histo.pdf:/Users/camilorl/Zotero/storage/ULPZVWPD/Betancourth-Cundar et al. - 2020 - Genetic differentiation and overexploitation histo.pdf:application/pdf},
}

@article{oconnell_frank_2020,
	title = {Frank Beach Award Winner: Lessons from poison frogs on ecological drivers of behavioral diversification},
	volume = {126},
	issn = {0018506X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0018506X20301951},
	doi = {10.1016/j.yhbeh.2020.104869},
	shorttitle = {Frank Beach Award Winner},
	abstract = {Variation in natural behavior is tightly linked to the ecological resources with which they co-evolved. This review discusses poison frog behavior and neuroendocrinology to illustrate how ecological factors drive diversification of behavior and its underlying neural mechanisms. Poison frogs show tremendous diversity in reproductive strategies that are tightly linked to water resources in their environment. Different species utilize particular pool sizes to rear their offspring, which has selected for sex differences in parental behavior among poison frog species. Tadpole behavior reflects the behavioral diversity of adults, where tadpoles can display social group living or violent ag­ gression and begging behavior, which are all associated with pool size and occupancy. Using this behavioral diversity among poison frog species, we have identified core brain regions, like the hippocampus and preoptic area, as being involved in regulating different aspects of amphibian parental behavior. In contrast to core brain regions, the neu­ romodulators governing these behaviors seem to be more labile across species. This work exemplifies how com­ parative studies are a prime experimental system to study how evolution tunes neural circuits that give rise to the diversity of behaviors we observe in the natural world. Finally, this review ends on a more important form of diversity – that of our scientific community – and how community outreach, decolonization of field based science, and inclusion of groups historically excluded from conducting research are needed for the scientific enterprise to transform into something truly beneficial for all members of our society.},
	pages = {104869},
	journaltitle = {Hormones and Behavior},
	shortjournal = {Hormones and Behavior},
	author = {O'Connell, Lauren A.},
	urldate = {2021-10-19},
	date = {2020-11},
	langid = {english},
	file = {O'Connell - 2020 - Frank Beach Award Winner Lessons from poison frog.pdf:/Users/camilorl/Zotero/storage/K5MBULK7/O'Connell - 2020 - Frank Beach Award Winner Lessons from poison frog.pdf:application/pdf},
}

