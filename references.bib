	author = {Spande, Thomas F. and Jain, Poonam and Garraffo, H. Martin and Pannell, Lewis K. and Yeh, Herman J. C. and Daly, John W. and Fukumoto, Sinji and Imamura, Kohji and Tokuyama, Takashi and Torres, Juan A. and Snelling, Roy R. and Jones, Tappey H.},
	urldate = {2025-01-17},
