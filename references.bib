	langid = {english},
	file = {Trainor et al. - 2006 - Estrogenic encounters How interactions between ar.pdf:/Users/camilorl/Zotero/storage/NV39TTTX/Trainor et al. - 2006 - Estrogenic encounters How interactions between ar.pdf:application/pdf},
}

@article{ros_role_2004,
