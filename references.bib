@article{brown_measurement_1989,
	title = {{THE} {MEASUREMENT} {OF} {VOCAL} {AMPLITUDE} {AND} {VOCAL} {RADIATION} {PATTERN} {IN} {BLUE} {MONKEYS} {AND} {GREY}-{CHEEKED} {MANGABEYS}},
