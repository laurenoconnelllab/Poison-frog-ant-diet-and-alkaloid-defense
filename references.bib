	pmcid = {PMC2930265},
	file = {PubMed Central Full Text PDF:/Users/camilorl/Zotero/storage/WZVGZVWH/Love and Bee - 2010 - An experimental test of noise-dependent voice ampl.pdf:application/pdf},
}

