@article{wilczynski_acoustic_1988,
	title = {Acoustic cues mediate inter-male spacing in a neotropical frog},
	volume = {36},
	issn = {00033472},
	doi = {10.1016/S0003-3472(88)80065-4},
	abstract = {Males of the neotropical anuran species Eleutherodactylus diastema produce a high-frequency, tonal, advertisement call when aggregated into breeding choruses. Within the chorus, males occupy elevated call sites scattered through large forest patches. The hypothesis that males use the amplitude of their neighbours' calls as a proximate cue for positioning themselves, was tested. Call characteristics were analysed, inter-male distances and neighbour call amplitudes were measured, and perch availability was assessed. Males were non-randomly distributed within the aggregation and perch availability did not determine inter-male spacing. Neighbour call amplitudes were kept within a narrow range, and relative variability in neighbour call amplitude was much, less than variability in inter-male distances. These results support the hypothesis that males in this species use the amplitude of neighbours' calls as the proximate cue for maintaining inter-male distance, and hence the spatial organization of the chorus. © 1988.},
