@article{agrawal_toxic_2012,
	title = {Toxic cardenolides: chemical ecology and coevolution of specialized plant–herbivore interactions},
	volume = {194},
	rights = {http://onlinelibrary.wiley.com/{termsAndConditions}\#vor},
	issn = {0028-646X, 1469-8137},
	url = {https://nph.onlinelibrary.wiley.com/doi/10.1111/j.1469-8137.2011.04049.x},
