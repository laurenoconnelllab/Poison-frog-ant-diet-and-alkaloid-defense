	shortjournal = {Zootaxa},
	author = {Fouquet, Antoine and Vacher, Jean-Pierre and Courtois, Elodie A. and Villette, Benoit and Reizine, Hugo and Gaucher, Philippe and Jairam, Rawien and Ouboter, Paul and Kok, Philippe J. R.},
	urldate = {2021-12-04},
