	langid = {english},
	file = {Ros et al. - 2004 - The role of androgens in the trade-off between ter.pdf:/Users/camilorl/Zotero/storage/WJWJRCLS/Ros et al. - 2004 - The role of androgens in the trade-off between ter.pdf:application/pdf},
}

