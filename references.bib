@article{rojas_habitat_2019,
	title = {From habitat use to social behavior: natural history of a voiceless poison frog, \textit{Dendrobates tinctorius}},
	volume = {7},
	issn = {2167-8359},
	url = {https://peerj.com/articles/7648},
	doi = {10.7717/peerj.7648},
	shorttitle = {From habitat use to social behavior},
	abstract = {Descriptive studies of natural history have always been a source of knowledge on which experimental work and scientiﬁc progress rely. Poison frogs are a well-studied group of small Neotropical frogs with diverse parental behaviors, distinct calls, and bright colors that warn predators about their toxicity; and a showcase of advances in fundamental biology through natural history observations. The dyeing poison frog, Dendrobates tinctorius, is emblematic of the Guianas region, widespread in the pet trade, and increasingly popular in research. This species shows several unusual behaviors, such as the lack of advertisement calls and the aggregation around tree-fall gaps, which remain poorly described and understood. Here, we summarize our observations from a natural population of D. tinctorius in French Guiana collected over various ﬁeld trips between 2009 and 2017; our aim is to provide groundwork for future fundamental and applied research spanning parental care, animal dispersal, disease spread, habitat use in relation to color patterns, and intra-speciﬁc communication, to name a few. We report sex differences in habitat use and the striking invasion of tree-fall gaps; describe their courtship and aggressive behaviors; document egg development and tadpole transport; and discuss how the knowledge generated by this study could set the grounds for further research on the behavior, ecology, and conservation of this species.},
	pages = {e7648},
	journaltitle = {{PeerJ}},
	author = {Rojas, Bibiana and Pašukonis, Andrius},
	urldate = {2021-11-01},
