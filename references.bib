	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2006.00977.x},
	keywords = {Abundance, competition, functional homogenization, {GAM} modelling, interaction milieu, niche theory},
	file = {Julliard et al. - 2006 - Spatial segregation of specialists and generalists.pdf:/Users/camilorl/Zotero/storage/SHWRKCTF/Julliard et al. - 2006 - Spatial segregation of specialists and generalists.pdf:application/pdf;Snapshot:/Users/camilorl/Zotero/storage/X8DX9QNC/j.1461-0248.2006.00977.html:text/html},
