	file = {Giuggioli et al. - 2011 - Animal Interactions and the Emergence of Territori.pdf:/Users/camilorl/Zotero/storage/TSY3IXFQ/Giuggioli et al. - 2011 - Animal Interactions and the Emergence of Territori.pdf:application/pdf},
